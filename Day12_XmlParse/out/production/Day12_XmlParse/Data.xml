<?xml version="1.0" encoding="utf-8" ?>
<students>
    <!--
    students是这个xml文件最外层的标签
    是这个文档的根元素
    一个xml文档中只能有一个根元素

    <xxx></xxx>
    <xxx>:开始标签
    </xxx>:结束标签
    <-->
    <student name="张三" >
        <!--
        Student这个标签，称为students的子标签
        name称为：Student标签的属性
        "张三"：name属性的值
        《-->

        <!--score 是Student子标签
        200是标签中的文本
        <-->
        <score>200</score>
    </student>

    <student name="李四">
        <score>1000</score>
    </student>
</students>

